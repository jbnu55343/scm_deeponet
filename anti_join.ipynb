    "import pandas as pd\n",
    "orig = pd.read_csv('customers.csv')                # 原始点\n",
    "snap = pd.read_csv('customers_snapped.csv')        # 已吸附点\n",
    "unmatched = orig[~orig['id'].isin(snap['id'])]     # 不在 snapped 的就是未吸附\n",
    "unmatched.to_csv('customers_unmatched.csv', index=False)\n"
   ]
